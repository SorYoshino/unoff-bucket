{
    "version": "2.7.60",
    "description": "All-in-one collaboration platform for API documentation, API debugging, API Mock and API test automation.",
    "homepage": "https://apifox.com",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://file-assets.apifox.com/download/2.7.60/Apifox-2.7.60.exe",
            "hash": "sha512:fa3816c7f2a2463aa87a54c8f1c0e9d06daaa5e6fb36a8257114eeec3052f29cb2207e6cb97b724cf905b3bb1fae77f605f346b2c8d163ba78db2b3428cbd34c"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive -Path \"$dir\\$fname\" -DestinationPath \"$dir\\tmp\" -ExtractDir '$PLUGINSDIR' -Removal",
            "Get-ChildItem -Path \"$dir\\tmp\" -Filter '*.7z' | Expand-7zipArchive -DestinationPath $dir -Removal",
            "Remove-Item -Path \"$dir\\tmp\" -Recurse -Force -ErrorAction SilentlyContinue"
        ]
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox",
            "--user-data-dir=\"$dir\\UserData\""
        ]
    ],
    "persist": "UserData",
    "pre_uninstall": "Stop-Process -Name 'ApifoxAppAgent' -Force -ErrorAction SilentlyContinue",
    "checkver": {
        "url": "https://docs.apifox.com/changelog.md",
        "regex": "## ([\\d.]+)(?=\\s)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://file-assets.apifox.com/download/$version/Apifox-$version.exe",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
}
