        "$remove_list | Remove-Item -Force -Recurse -ErrorAction SilentlyContinue"
    ],
    "psmodule": {
        "name": "PSFzf"
    },
    "checkver": {
        "url": "https://www.powershellgallery.com/packages/PSFzf",
        "regex": "<h2>([\\d.]+)</h2>"
    },
    "autoupdate": {
        "url": "https://cdn.powershellgallery.com/packages/psfzf.$version.nupkg"
    }
}
