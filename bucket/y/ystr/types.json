        "64a79982140364da2411d2d03513411cfd1a717a686a62f8de807e9bdb9db4a2"
    ],
    "bin": [
        "Types.exe",
        "CTypes.exe"
    ],
    "shortcuts": [
        [
            "Types.exe",
            "Types"
        ]
    ],
    "checkver": {
        "script": [
            "$tmp_dir = \"$cachedir\\tmp\"",
            "$download_url = 'https://ystr.github.io/types/Types.zip'",
            "$download_path = cache_path $app 'unknown' $download_url",
            "New-Item -Path $tmp_dir -ItemType Directory -Force | Out-Null",
            "Invoke-WebRequest -Uri $download_url -OutFile $download_path -UseBasicParsing",
            "Expand-Archive -LiteralPath $download_path -DestinationPath $tmp_dir -Force",
            "$version_info = [System.Diagnostics.FileVersionInfo]::GetVersionInfo(\"$tmp_dir\\Types.exe\")",
            "Move-Item -Path $download_path -Destination (cache_path $app $version_info.ProductVersion $download_url) -Force",
            "Remove-Item -Path $tmp_dir -Recurse -Force -ErrorAction SilentlyContinue",
            "Write-Output $version_info.ProductVersion"
        ],
        "regex": "([\\d.]+)"
    },
    "autoupdate": {
        "url": [
            "https://ystr.github.io/types/Types.zip",
            "https://ystr.github.io/types/CTypes.zip"
        ]
    }
}
