    "pre_install": [
        "$remove_list = @('_rels', 'package', '*.nuspec', '*Content*.xml') | ForEach-Object -Process { \"$dir\\$_\" }",
        "$remove_list | Remove-Item -Recurse -Force -ErrorAction SilentlyContinue"
    ],
    "psmodule": {
        "name": "z"
    },
    "checkver": {
        "url": "https://www.powershellgallery.com/packages/z",
        "regex": "<h2>([\\d.]+)</h2>"
    },
    "autoupdate": {
        "url": "https://cdn.powershellgallery.com/packages/z.$version.nupkg"
    }
}
