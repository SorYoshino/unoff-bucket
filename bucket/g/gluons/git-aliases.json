{
    "version": "0.3.8",
    "description": "A PowerShell module that provides partial Git aliases from Oh My Zsh's git plugin.",
    "homepage": "https://github.com/gluons/powershell-git-aliases",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/gluons/powershell-git-aliases/blob/HEAD/LICENSE"
    },
    "notes": [
        "Use this module by executing: `Import-Module -Name git-aliases -DisableNameChecking`.",
        "Add the above command to your $PROFILE to enable automatic loading in future sessions."
    ],
    "suggest": {
        "git": [
            "main/git",
            "main/git-with-openssh"
        ]
    },
    "url": "https://cdn.powershellgallery.com/packages/git-aliases.0.3.8.nupkg",
    "hash": "9463cfe2d4c4c1cf8e3e73a687fed5bd34da6a9f736fd8a8cf071430f30c3f07",
    "pre_install": [
        "$remove_list = @(",
        "    '_rels', 'package',",
        "    '*.nuspec', '*Content*.xml'",
        ") | ForEach-Object -Process { \"$dir\\$_\"}",
        "$remove_list | Remove-Item -Recurse -Force -ErrorAction SilentlyContinue"
    ],
    "psmodule": {
        "name": "git-aliases"
    },
    "checkver": {
        "url": "https://www.powershellgallery.com/packages/git-aliases",
        "regex": "<h2>([\\d.]+)</h2>"
    },
    "autoupdate": {
        "url": "https://cdn.powershellgallery.com/packages/git-aliases.$version.nupkg"
    }
}
