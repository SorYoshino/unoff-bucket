{
    "version": "25.12-4262",
    "description": "A music player developed by the KDE community, focused on simplicity, usability, and a pleasant listening experience.",
    "homepage": "https://apps.kde.org/elisa/",
    "license": {
        "identifier": "LGPL-3.0-or-later",
        "url": "https://invent.kde.org/multimedia/elisa/-/blob/HEAD/COPYING"
    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.kde.org/ci-builds/multimedia/elisa/release-25.12/windows/elisa-release_25.12-4262-windows-cl-msvc2022-x86_64.7z",
            "hash": "281cba5626d6e5256c0388c6d9366e55bdcb4462b917b235728fccb144a8c6b6"
        }
    },
    "bin": "bin\\elisa.exe",
    "shortcuts": [
        [
            "bin\\elisa.exe",
            "Elisa"
        ]
    ],
    "checkver": {
        "script": [
            "$app_type = 'multimedia'",
            "$path_filter = 'release-*/'",
            "$releases_url = 'https://cdn.kde.org/ci-builds/{1}/{0}/' -f $app, $app_type",
            "$version_sort = { if ($_.href -match '([\\d.]+)/$') { [version]$Matches[1] } }",
            "$releases_page = Invoke-WebRequest -Uri $releases_url -UseBasicParsing -ErrorAction Stop",
            "$path_segments = $releases_page.links | Where-Object { $_.href -like $path_filter } |",
            "        Sort-Object -Property $version_sort -Descending | ForEach-Object -MemberName href",
            "foreach ($path_segment in $path_segments.TrimEnd('/')) {",
            "    $release_url = 'https://cdn.kde.org/ci-builds/{1}/{0}/{2}/windows/' -f $app, $app_type, $path_segment",
            "    $download_page = Invoke-WebRequest -Uri $release_url -UseBasicParsing -ErrorAction Stop",
            "    $archive_name = $download_page.Links | Where-Object { $_.href -match '\\.7z$' } | Select-Object -ExpandProperty href -First 1",
            "    if ($null -ne $archive_name) { Write-Output $archive_name; break }",
            "}"
        ],
        "regex": "elisa-release_([\\d.-]+)-windows-cl-(?<lib>\\w+)-x86_64\\.7z"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.kde.org/ci-builds/multimedia/elisa/release-$matchHead/windows/elisa-release_$version-windows-cl-$matchLib-x86_64.7z",
                "hash": {
                    "url": "$url.sha256"
                }
            }
        }
    }
}
